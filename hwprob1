import random

a = []
for i in range(11):
	x = random.randint(0,100)
	a.append(x)
print(a)
q = 0
while q < len(a):
	if a[q]%3 is 0:
		del a[q]
		q-=1
	q += 1
z = 1
c = 0
while z < len(a):
	for c in range(len(a)):
		if a[(-1*z)] >= a[c]:
			a.insert(c-1, a[-1*z])
			del a[(-1*z)]
			z-=1
		z+= 1
	print(a)

