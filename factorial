def add(a,b):
	return (a+b)
print(add(5,7))
x = add(3,4)
print(x)

import sys
a = int(sys.argv[1])
q = a
i = a

while a > 1:
	i *= (a-1)
	a -= 1

print(i)

def fact(n):
	if n is 1:
		return 1
	return n*fact(n-1)
print(fact(2))

